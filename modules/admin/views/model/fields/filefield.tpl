<div class="field-container filefield-div  {if isset($htmlCss.class)}{$htmlCss.class}{/if}">	<div class="left field-title">		{$title} {if isset($lang) && !empty($lang)}[{$lang}]{/if}	</div>	<div class="right">		<input type="hidden" name="{$name}" class="file-field-value" />		<a href="{$public_url}{$value}" target="_blank" >view</a>		<a href="image-field">open manager</a>		<div class="fm-main-container">			<div id="filemanager-container" class="">				<div id="fm-entry">					{$entries}				</div>				<div><a href="#" class="create-folder">create folder</a> <input type="text" id="folder-name" /></div>				<div>				<div><input type="file" id="file-to-upload" /></div>				</div>			</div>			{literal}				<script>					FileManager.init({						item: '.fm-file',						action: function() {							$(this).parents('.fm-main-container').parent().find('.file-field-value').val($(this).parents('.fm-item').attr('path'));						}					});				</script>			{/literal}		</div>	</div>	<div class="clear"></div></div>