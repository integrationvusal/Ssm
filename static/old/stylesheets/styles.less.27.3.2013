}
/* Menu for 767 end */

/* menu end */

/* banners slider */
#banner-slider-container {
	margin-bottom: 3%;
	margin-top: 2%;
	#banner-slider-left, 
	#banner-slider-middle, 
	#banner-slider-right {
		.left;
	}
	#banner-slider-left,  
	#banner-slider-right {
		width: 4%;
	}
	#banner-slider-middle {
		width: 90%;
		margin-left: 1%;
		margin-right: 1%;
		.mslider-container {
			.mslider-top {
				width: 90%;
				margin-left: 5%;
				margin-right: 5%;
			}
		}
	}
	#banner-slider-left, #banner-slider-right {
		padding-top: 3%;
		ul {
			margin: 0;
			padding: 0;
			list-style: none;
			li {
				margin-top: 30%;
				a {
					.a-rhover;
					color: #ff4e00;
					img {
						width: 30%;
						min-width: 10px;
						max-width: 11px;
					}
				}
				.lang-item-active {
					text-decoration: none;
					color: #656565;
				}
			}
		}
	}
	#banner-slider-left {
		text-align: right;
	}
	#banner-slider-right {
		text-align: left;
	}
}
/* banners slider end */

/* calculator */
#object-discount-value {
	.color-green;
}
#calculator-container {
	.color-gray;
	margin-bottom: 2%;
	#calculator-title {
		margin-bottom: 5px;
	}
	#calculator-left, #calculator-right {
		padding-left: 3%;
		padding-right: 3%;
		width: 44%;
	}
	#calculator-left {
		.left;
		#calculator-left-title {
			width: 60%;
			.left;
		}
		#category-select-container {
			.left;
			width: 40%;
			.select-container {
				width: 84%;
				margin-left: 0 !important;
				.selected-value-pointer {
					width: 8%;
				}
				.select-options-container {
					.z(999);
				}
			}
		}
	}
	#calculator-right {
		.right;
		#category-right-title, #object-select-container {
			.left;
		}
		#category-right-title {
			.bold;
			margin-top: 1%;
		}
		#calculator-result {
			.left;
			.bold;
			margin-top: 1%;
			margin-left: 5%;
		}
		#object-select-container {
			width: 37%;
			.select-container {
				margin-left: 1% !important;
				.selected-value-pointer {
					width: 8.2%;
					margin-top: 5.2%;
				}
			}
		}
		.inactive-object-select {
			.select-container {
				.selected-value {
					color: #ccc;
				}
				.selected-value-pointer {
					opacity: 0.5;
				}
			}
		}
	}
	.red-border {
		border-bottom-color: @colorRed !important;
		border-bottom: 1px dotted;
	}
