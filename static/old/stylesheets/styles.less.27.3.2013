.content-child-elements {
	.z(999);
	width: 250px;
	top: 30%;
	left: -215px;
	.content-child-elements-left, .content-child-elements-right {
		.left;
	}
	.content-child-elements-left {
		background-color: #fff;
		box-shadow: 0 3px 4px #CCCCCC;
		width: 77%;
		padding-left: 4%;
