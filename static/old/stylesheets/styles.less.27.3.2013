/* less functions */
@colorGreen: #0185c7;
@colorGray: #656565;
@colorRed: #ff4e00;
@colorWhite: #fff;
@footerBG: #829dad;
@inputBorderColor: #c4c5a6;

.bgImage(@image) {
	background-image: url("../img/@{image}");
}
.aHover(@color) {
	color: @color !important;
	text-decoration: underline !important;
	&:hover {
		text-decoration: none !important;
	}
}
.aRHover(@color) {
	color: @color !important;
	text-decoration: none !important;
	&:hover {
		text-decoration: underline !important;
	}
}
.bgColor(@color) {
	background-color: @color;
}
.z(@num) {
	z-index: @num;
}
/* less functions end */

/* Common Styles */
html {
	height: 100%;
    overflow-y: scroll;
}
form,li {
	margin: 0;
	padding: 0;
	border: 0;
}
body {
	height: 100%;
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-family: Arial;
	background-color: #e8e8e8;
	.bgImage("body-bg.jpg");
	background-repeat: repeat-y;
	background-position: center 0;
	background-size: 100% auto;
}
img {
	border: 0;
}
.left {
    float: left !important;
}
.right {
    float: right !important;
}
.text-right {
	text-align: right;
}
.clear {
    clear: both;
}
.hide {
    display: none;
}
.hidden {
	visibility: hidden;
}
.show {
	display: block;
}
.fixed {
	position: fixed;
}
.absolute {
	position: absolute;
}
.relative {
	position: relative;
}
.bold {
	font-weight: bold;
}
.italic {
	font-style: italic;
}
.ul-none {
	list-style: none;
}
.inline {
	display: inline;
}
.bold {
	font-weight: bold;
}
.a-rhover {
	text-decoration: underline;
	&:hover {
		text-decoration: none;
	}
}
.a-hover {
	text-decoration: none;
	&:hover {
		text-decoration: underline;
	}
}
.width100 {
	width: 100%;
}
.height100 {
	height: 100%;
}
.margin-problem {
	padding: 1px;
}
.center-container {
	width: 90%;
	margin-left: 5%;
	margin-right: 5%;
}

/* fonts */
.font25 {
	font-size: 25px;
}
.font30 {
	font-size: 30px;
}
.font20 {
	font-size: 20px;
}
.font17 {
	font-size: 17px;
}
.font16 {
	font-size: 16px;
}
.font18 {
	font-size: 18px;
}
.font15 {
	font-size: 15px;
}
.font33 {
	font-size: 33px;
}
.font27 {
	font-size: 27px;
}
.font13 {
	font-size: 13px;
}
/* fonts end */

/* text colors */
.color-green {
	color: @colorGreen;
}
.color-gray {
	color: @colorGray;
}
.color-red {
	color: @colorRed;
}
.color-white {
	color: @colorWhite;
}
/* text colors end */

/* Common styles end */

/* Controllers */
/* Select */
.select-container {
	width: 86%;
	border: 1px solid @inputBorderColor;
	border-radius: 5px;
	background-color: #fff;
	padding: 2% 8%;
	margin-left: 10%;
	cursor: pointer;
	.select-options-container {
		.width100;
		z-index: 20;
		left: -1px;
		background-color: #fff;
		border: 1px solid @inputBorderColor;
		border-top: none;
		border-radius: 0 0 5px 5px;
		.select-option {
			width: 93%;
			padding-left: 7%;
			padding-top: 1%;
			padding-bottom: 1%;
			&:hover {
				background-color: #eee;
			}
		}
	}
	.selected-value-container {
		.width100;
		.selected-value,
		.selected-value-pointer {
			.left;
		}
		.selected-value {
			width: 90%;
		}
		.selected-value-pointer {
			width: 10%;
			margin-top: 4%;
			img {
				.width100;
			}
		}
	}
}
/* Select end */

/* Slider */

.horizontal-slider-container {
	.width100;
	.horizontal-slider-to-left, 
	.horizontal-slider-to-right {
		width: 2%;
		height: 100%;
		cursor: pointer;
		margin-top: 4%;
		img {
			min-width: 8px;
			width: 50%;
		}
	}
	.horizontal-slider-to-left {
		margin-left: 0.8%;
	}
	.horizontal-slider-to-left, 
	.horizontal-slider-to-right, 
	.horizontal-slider-wrapper {
		.left;
	}
	.horizontal-slider-wrapper {
		width: 95%;
		/*65%*/
		overflow: hidden;
	}
	.horizontal-slider-ribbon {
		width: 1000%;
		.horizontal-slider-item {
			.left;
			width: 3.32%;
			/*5%*/
			.horizontal-slider-item-image {
				width: 80%;
				margin-left: 10%;
				margin-right: 10%;
				height: 105px;
				display: table;
				img {
					width: 70%;
					margin-left: 15%;
				}
				.horizontal-slider-item-image-cell {
					display: table-cell;
					vertical-align: middle;
				}
			}
			.horizontal-slider-item-title {
				width: 80%;
				text-align: center;
				margin-left: 10%;
				margin-right: 10%;
				margin-top: 5%;
				a {
					.a-rhover;
					.color-green;
				}
			}
		}
	}
}

/* Slider end */

/* MSlider */
.mslider-container {
	.mslider-top {
		.relative;
		margin-bottom: 2%;
		padding-top: 1%;
		.mslider-pointer {
			.absolute;
			top: 20%;
			width: 7%;
			cursor: pointer;
			img {
				.width100;
			}
		}
		.mslider-segment {
			.left;
			width: 7%;
			.mslider-segment-line,
			.mslider-segment-pointer {
				.left;
			}
			.mslider-segment-line {
				width: 100%;
				height: 1px;
				background-color: #000;
			}
			.mslider-segment-pointer {
				height: 3px;
				width: 1px;
				background-color: #000;
				position: relative;
				top: -2px;
			}
		}
	}
	.mslider-bottom {
		.mslider-wrapper {
			overflow: hidden;
			.mslider-ribbon {
				width: 1000%;
				.mslider-item {
					.left;
					border-radius: 10px;
					overflow: hidden;
					width: 10%;
					img {
						.width100;
					}
					.text-container {
						.absolute;
						.width100;
						left: 0;
						top: 0;
						.text-layer {
							width: 70%;
							color: #EAF0B1;
							margin-top: 6%;
							text-align: left;
						}
						.text-left {
							margin-left: 3%;
						}
						.text-right {
							margin-left: 27%;
						}
						.text-center {
							margin-left: 15%;
						}
					}
				}
			}
		}
	}
}
/* MSlider end */

/* Controllers end */

/* main wrapper */
.main-wrapper {
	width: 90%;
	margin-left: 5%;
	margin-right: 5%;
}

/* header */
#header {
	.width100;
	#logo-container {
		.left;
		width: 16%;
		margin-left: 4%;
		img {
			min-width: 100px;
			.width100;
		}
	}
	#header-info-container {
		.right;
		width: 35%;
		text-align: left;
		margin-top: 20px;
		#info-phone {
			.color-gray;
			.bold;
			margin-bottom: 2%;
		}
		#info-links {
			width: 70%;
			.right;
			.info-left-link {
				.left;
				width: 25%;
				img {
					.width100;
				}
			}
			.info-right-link {
				.left;
				margin-top: 20px;
				margin-left: 10%;
			}
			a {
				border-bottom: 1px dotted #FE4F00;
				.color-green;
				border-bottom: none;
				&:hover {
					text-decoration: none;
				}
			}
			& > a {
				margin-right: 13%;
				margin-left: 1%;
			}
		}
	}
}
/* header end */

/* menu */
#menu-container {
	.width100;
	.relative;
	img {
		.width100;
	}
	#menu-content {
		.absolute;
		.width100;
		left: 0;
		top: 25%;
		.menu-item , .menu-pointer {
			.left;
		}
		.menu-pointer {
			width: 1.3%;
			margin-top: 0.5%;
			img {
				.width100;
				max-width: 10px;
			}
			.p-inactive {
				.show;
			}
			.p-active {
				.hide;
			}
		}
		.menu-pointer-active {
			.p-inactive {
				.hide;
			}
			.p-active {
				.show;
			}
		}
		.menu-item {
			margin-left: 2%;
			margin-right: 2%;
			min-width: 11%;
			& > a {
				.aHover(@colorGray);
			}
		}
		.registration {
			width: 16%;
			padding-right: 0;
			img {
				width: 10%;
				max-width: 11px;
			}
			a {
				text-decoration: none;
				span {
					.aHover(@colorGray);
				}
				margin-right: 2%;
			}
		}
		.profile-container {
			width: 10%;
			margin-right: 1%;
			.profile-link {
				.left;
				width: 80%;
				a {
					.aHover(#656565);
				}
			}
			.profile-logout {
				.right;
				margin-top: 2%;
				width: 20%;
				a {
					img {
						.width100;
						max-width: 14px;
					}
				}
			}
		}
	}
	.submenu-1 {
		.bgColor(#747474);
		.z(99);
		width: 170%;
		left: -40%;
		top: 195%;
		padding: 5%;
		border-radius: 0 0 10px 10px;
		border-left: 1px solid #c39b89;
		border-right: 1px solid #c39b89;
		border-bottom: 1px solid #c39b89;
		.submenu-item {
			margin-top: 2%;
			padding-left: 6%;
			.text {
				.left;
				width: 90%;
				a {
					.aRHover(#fff);
					&:hover {
						font-style: italic;
					}
				}
			}
			.pointer {
				.left;
				width: 10%;
				margin-top: 3%;
				img {
					padding-right: 3%;
					width: auto;
				}
			}
		}
	}
}

