			.width100;
			height: 20%;
			#partners-icon, #partners-block-title {
				.left;
			}
			#partners-icon {
				width: 5%;
				img {
					
					max-width: 30px;
				}
			}
			#partners-block-title {
				width: 29%;
				margin-bottom: 2%;
				margin-top: 4%;
				margin-left: 4%;
			}
		}
		#partners-slider-container-bottom {
			.bgColor(#fff);
			width: 98%;
			padding-top: 3%;
			padding-left: 1%;
			padding-right: 1%;
			padding-bottom: 2%;
			height: 80%;
			border-radius: 10px;
			box-shadow: 1px 2px 2px #666666;
			border-top: 1px solid #eee;
		}
	}
}

.pockets-table-2 {
	tr {
		td {
			img {
				width: 50% !important;
				height: auto !important;
			}
		}
	}
}
.pockets-table-3 {
	tr {
		td {
			img {
				width: 80% !important;
				height: auto !important;
			}
		}
	}
}
.pockets-table-p {
	tr {
		td {
