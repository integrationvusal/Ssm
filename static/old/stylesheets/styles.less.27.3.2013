/* content end */

/* main wrapper end */

/* footer */
	
#footer-container {
	.width100;
	background-color: @footerBG;
	padding-bottom: 5%;
	.bgImage("footer-bg.jpg");
	background-repeat: no-repeat;
	background-position: center 0;
	margin-top: 3%;
	#top-gradient {
		.bgImage("footer-top-gradient.gif");
		height: 31px;
	}
	#search-container {
		width: 37%;
		margin-left: 31.5%;
		margin-right: 31.5%;
		margin-top: 3%;
		#search-input {
			width: 77%;
			input {
				.absolute;
				.color-gray;
				.input-box;
				left: 0;
