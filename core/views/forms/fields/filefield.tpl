<div class="form-field-container filefield-div {$cssClasses}" id="el-{$randNum}">	<div class="left form-field-title">		{$title}:	</div>	<div class="right form-field-own">		<div class="left form-file-field-button"><input type="button" class="std-button" value="{$messages.common.browse}" /></div>		<div class="left form-file-field-value"></div>		<div class="clear"></div>		<input type="file" class="hide form-file-field-item" name="{$name}" />	</div>	<div class="clear"></div></div><script>	$('#el-{$randNum}').find('.std-button').click(function(){		$('#el-{$randNum}').find('.form-file-field-item').click();	});	$('#el-{$randNum}').find('.form-file-field-item').change(function(){		$('#el-{$randNum}').find('.form-file-field-value').html($(this).val());	});</script>